{
  "id": "AdvancedInstallerTool",
  "publisher": "Caphyon",
  "name": "Advanced Installer Tool Installer",
  "version": "1.0.0",
  "description": "Acquires a specific version of Advanced Installer from internet or the tools cache and adds it to the PATH.",
  "tags": [
    "build",
    "release",
    "download",
    "setup",
    "advanced installer",
    "tool installer"
  ],
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    }
  ],
  "categories": [
    "Build and release"
  ],
  "branding": {
    "color": "rgb(239, 237, 233)",
    "theme": "light"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.txt"
    }
  },
  "files": [
    {
      "path": "AdvinstTool"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "icons": {
    "default": "images/advinst-logo.png"
  },
  "links": {
    "home": {
      "uri": "https://github.com/caphyon/advinst-vsts-tool"
    },
    "repository": {
      "uri": "https://github.com/caphyon/advinst-vsts-tool"
    },
    "issues": {
      "uri": "https://github.com/caphyon/advinst-vsts-tool/issues"
    },
    "support": {
      "uri": "http://www.advancedinstaller.com/support.html"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/caphyon/advinst-vsts-tool"
  },
  
  "galleryFlags": [
    "Private"
  ],
  "contributions": [
    {
      "id": "Caphyon.AdvancedInstaller.Tool",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "AdvinstTool"
      }
    }
  ]
}