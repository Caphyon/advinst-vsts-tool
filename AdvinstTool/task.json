{
  "id": "ad40e839-f5ce-4ef5-a418-d2203b0dad2b",
  "name": "AdvancedInstallerTool",
  "friendlyName": "Advanced Installer Tool",
  "description": "Downloads Advanced Installer tool",
  "author": "Caphyon",
  "version": {
    "Major": 1,
    "Minor": 0,
    "Patch": 0
  },
  "category": "Tool",
  "runsOn": [
    "Agent",
    "DeploymentGroup"
  ],
  "preview": "true",
  "satisfies": [
    "AdvancedInstaller"
  ],
  "minimumAgentVersion": "2.115.0",
  "instanceNameFormat": "Use Advanced Installer $(advinstVersion)",
  "inputs": [
    {
      "name": "advinstVersion",
      "type": "string",
      "label": "Version",
      "required": true,
      "helpMarkDown": "A version that specifies the Advanced Installer version to make available on the PATH. If no version is specified the latest available will be used."
    },
    {
      "name": "advinstLicense",
      "type": "password",
      "label": "License Key",
      "required": false,
      "helpMarkDown": "Advanced Installer license that will be used for registration."
    }
  ],
  "execution": {
    "Node": {
      "target": "AdvinstTool.js"
    }
  },
  "postjobexecution": {
    "Node": {
      "target": "AdvinstToolCleanup.js"
    }
  }
}